  "license": "MIT",
  "private": true,
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "browser-run": "^3.1.1",
    "browserify": "^13.0.1",
    "faucet": "0.0.1",
    "node-static": "^0.7.7",
    "blue-tape": "^0.2.0"
   }
}
